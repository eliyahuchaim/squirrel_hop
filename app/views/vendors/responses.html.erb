<div class="row">
<% @responses.each do |response| %>
<div class="col m4">
<div class="card">
  <div class="card-image">
    <img src="https://static.pexels.com/photos/33109/fall-autumn-red-season.jpg">
    <span class="card-title"><%= response.post.title %></span>
    <!-- <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">+</i></a> -->
  </div>
  <div class="card-content">
    <p><%= response.post.description%></p>
  </div>
  <div class="card-action">
    <div class="row">
    Price Quote: $<%= response.price_quote %>
    <div class="waves-effect waves-light btn red" style="float:right">
        <%= button_to "Delete", response_path(response), method:'delete'%>
    </div>
    </div>
  </div>

</div>
</div>

<% end %>
</div>
