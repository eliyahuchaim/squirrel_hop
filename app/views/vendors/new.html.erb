<div class="container">
<div align="center"><h2>Create New Vendor</h2></div><br>

<%= form_for @vendor do |f| %>
<div class="input-field">
<%= f.label :username %>
<%= f.text_field :username %>
</div>
<div class="input-field">
<%= f.label :password %>
<%= f.password_field :password %>
</div>
<div class="input-field">
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
</div>
<div class="input-field">
<%= f.label :company_name %>
<%= f.text_field :company_name %>
</div>
<div class="input-field">
<%= f.label :email %>
<%= f.text_field :email %>
</div>
<br>
<br>
<div class="waves-effect waves-light btn"> <%= f.submit "Create Account"%> </div>
<% end %>

</div>
