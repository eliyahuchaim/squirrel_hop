<h1>Log in to your account</h1>

<%= form_tag '/vendors/login' do %>
<%= label_tag :username %>
<%= text_field_tag :username %>
<%= label_tag :password %>
<%= password_field_tag :password %>
<%= submit_tag "Login" %>
<% end %>

<h3>Don't have an account? Sign up now!</h3>
<%= link_to "Create New Account", new_vendor_path %>
