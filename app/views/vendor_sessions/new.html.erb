<div class="container">
<h2>Vendor Login</h2><br>

<%= flash[:notice] %>
<%= form_tag '/vendors/login' do %>
<div class="input-field">
<%= label_tag :username %>
<%= text_field_tag :username %>
</div>
<div class="input-field">
<%= label_tag :password %>
<%= password_field_tag :password %>
</div>
<div class="row">
<div class="waves-effect waves-light btn"> <%= submit_tag "Login" %> </div>
Don't have an account? <%= link_to "Sign Up!", new_vendor_path %>
</div>
<% end %>

</div>
